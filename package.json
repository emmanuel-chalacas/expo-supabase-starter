{
  "name": "expo-supabase-starter",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "web": "expo start --web",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "lint": "eslint . --fix",
    "generate-colors": "node ./scripts/generate-colors.js && eslint ./constants/colors.ts --fix",
    "supa:login": "supabase login",
    "supa:init": "supabase init",
    "supa:link": "supabase link --project-ref %SUPABASE_PROJECT_REF%",
    "db:local:up": "supabase start",
    "db:local:reset": "supabase db reset",
    "db:local:verify": "node scripts/run-sql.js docs/sql/stage2-verify.sql",
    "db:migration:new": "supabase migration new",
    "db:push": "supabase db push",
    "db:remote:verify": "node scripts/run-sql.js docs/sql/stage2-verify.sql",
    "db:local:verify:stage3": "node scripts/run-sql.js docs/sql/stage3-verify.sql",
    "db:local:test:stage3": "node scripts/run-sql.js docs/sql/stage3-policy-tests.sql",
    "db:remote:verify:stage3": "node scripts/run-sql.js docs/sql/stage3-verify.sql",
    "db:remote:test:stage3": "node scripts/run-sql.js docs/sql/stage3-policy-tests.sql",
    "db:local:verify:stage4": "node scripts/run-sql.js docs/sql/stage4-verify.sql",
    "db:local:test:stage4": "node scripts/run-sql.js docs/sql/stage4-smoke-tests.sql",
    "db:remote:verify:stage4": "node scripts/run-sql.js docs/sql/stage4-verify.sql --remote",
    "db:remote:test:stage4": "node scripts/run-sql.js docs/sql/stage4-smoke-tests.sql --remote",
    "db:local:verify:stage5": "node scripts/run-sql.js docs/sql/stage5-status-verify.sql",
    "db:local:test:stage5": "node scripts/run-sql.js docs/sql/stage5-status-smoke-tests.sql",
    "db:remote:verify:stage5": "node scripts/run-sql.js docs/sql/stage5-status-verify.sql --remote",
    "db:remote:test:stage5": "node scripts/run-sql.js docs/sql/stage5-status-smoke-tests.sql --remote",
    "db:run:backfill:memberships": "node scripts/run-sql.js -e \"select public.fn_projects_membership_backfill();\"",
    "smoke:provider": "node scripts/provider-smoke.js"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4.6.3",
    "@hookform/resolvers": "^5.0.1",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@rn-primitives/label": "^1.1.0",
    "@rn-primitives/radio-group": "^1.1.0",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/switch": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@supabase/supabase-js": "^2.49.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "53.0.20",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "14.1.5",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "5.2.4",
    "expo-document-picker": "13.1.6",
    "expo-haptics": "~14.1.4",
    "expo-image": "2.4.0",
    "expo-linking": "~7.1.4",
    "expo-random": "^14.0.1",
    "expo-router": "5.1.4",
    "expo-secure-store": "~14.2.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "5.0.10",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.55.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-get-random-values": "^1.11.0",
    "react-native-haptic-feedback": "^2.0.3",
    "react-native-ios-context-menu": "^1.14.0",
    "react-native-context-menu-view": "^3.0.0",
    "react-native-reanimated": "~3.17.3",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "4.11.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.20.0",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "~3.4.17",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/aes-js": "^3.1.4",
    "@types/react": "~19.0.10",
    "dotenv": "^16.4.5",
    "eslint": "^9.0.0",
    "eslint-config-expo": "~9.2.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "pg": "^8.16.3",
    "prettier": "^3.5.3",
    "supabase": "^2.34.3",
    "toml": "^3.0.0",
    "typescript": "~5.8.3"
  },
  "expo": {
    "install": {
      "exclude": [
        "@supabase/supabase-js"
      ]
    }
  },
  "private": true,
  "packageManager": "yarn@1.22.22"
}
